{
    "name": "dynamic/silverstripe-locations",
    "description": "A Location DataObject for Silverstripe. Includes the ability to generate coordinates based on an address.",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "locations"
    ],
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "https://www.dynamicagency.com"
        }
    ],
    "require": {
        "dynamic/silverstripe-geocoder": "^3.0",
        "silverstripe/linkfield": "^4.0",
        "silverstripe/recipe-cms": "^5.0",
        "silverstripe/tagfield": "^3.0"
    },
    "require-dev": {
        "dnadesign/silverstripe-elemental": "^5",
        "silverstripe/recipe-testing": "^3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dynamic\\Locations\\": "src/",
            "Dynamic\\Locations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
